{
  "name": "dommodule",
  "version": "0.5.2",
  "description": "DOM assembler library",
  "files": [
    "lib"
  ],
  "main": "lib",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test nyc mocha",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "mocha": "mocha",
    "karma": "karma start",
    "test-full": "npm run karma && npm test",
    "build": "webpack",
    "webpack": "webpack -w",
    "api": "esdoc",
    "publish": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aristov/dommodule.git"
  },
  "keywords": [
    "DOM",
    "Attr",
    "CharacterData",
    "ChildNode",
    "Comment",
    "Document",
    "DocumentType",
    "DocumentFragment",
    "Element",
    "EventTarget",
    "Node",
    "ParentNode",
    "TargetAssembler",
    "Text",
    "attr",
    "comment",
    "document",
    "doctype",
    "fragment",
    "element",
    "text"
  ],
  "author": "Vyacheslav Aristov <vv.aristov@gmail.ru> (http://twitter.com/aristov7)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aristov/dommodule/issues"
  },
  "homepage": "https://github.com/aristov/dommodule#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.0",
    "esdoc": "^1.0.3",
    "esdoc-standard-plugin": "^1.0.0",
    "jsdom": "^11.3.0",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-opera-launcher": "^1.0.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-sinon": "^1.0.5",
    "mocha": "^5.0.1",
    "nyc": "^11.2.1",
    "sinon": "^4.0.1",
    "webpack": "^3.5.0"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false,
    "report-dir": "dist/coverage/jsdom"
  }
}
